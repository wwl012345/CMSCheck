{
    "website": "https://www.stackpath.com",
    "name": "StackPath",
    "matches": [
        {
            "search": "headers[x-backend-server]",
            "regexp": "hosting\\.stackcp\\.net$"
        },
        {
            "search": "headers[x-provided-by]",
            "offset": 1,
            "regexp": "^StackCDN(?: ([\\d.]+))?"
        }
    ]
}