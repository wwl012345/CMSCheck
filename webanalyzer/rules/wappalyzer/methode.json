{
    "website": "https://www.eidosmedia.com/",
    "name": "Methode",
    "matches": [
        {
            "regexp": "<!-- Methode uuid: \"[a-f\\d]+\" ?-->"
        },
        {
            "search": "meta[eomportal-uuid]",
            "regexp": "[a-f\\d]+"
        },
        {
            "search": "meta[eomportal-loid]",
            "regexp": "[\\d.]+"
        },
        {
            "search": "meta[eomportal-id]",
            "regexp": "\\d+"
        },
        {
            "search": "meta[eomportal-instanceid]",
            "regexp": "\\d+"
        },
        {
            "search": "meta[eomportal-lastUpdate]",
            "regexp": ".*"
        }
    ]
}