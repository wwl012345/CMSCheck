{
    "website": "https://www.pirobase-imperia.com/de/produkte/produktuebersicht/imperia-cms",
    "name": "imperia CMS",
    "implies": "Perl",
    "matches": [
        {
            "search": "meta[X-Imperia-Live-Info]",
            "regexp": ".*"
        },
        {
            "search": "meta[GENERATOR]",
            "offset": 1,
            "regexp": "^IMPERIA ([0-9.]{2,3})"
        },
        {
            "regexp": "<imp:live-info sysid=\"[0-9a-f-]+\"(?: node_id=\"[0-9/]*\")? *\\/>"
        }
    ]
}