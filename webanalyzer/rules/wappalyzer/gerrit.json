{
    "website": "http://www.gerritcodereview.com",
    "name": "Gerrit",
    "implies": [
        "Java",
        "git"
    ],
    "matches": [
        {
            "search": "meta[title]",
            "regexp": "^Gerrit Code Review$"
        },
        {
            "offset": 1,
            "regexp": ">Gerrit Code Review</a>\\s*\"\\s*\\(([0-9.]+)\\)"
        },
        {
            "regexp": "<(?:div|style) id=\"gerrit_"
        }
    ]
}