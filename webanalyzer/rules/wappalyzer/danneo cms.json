{
    "website": "http://danneo.com",
    "name": "Danneo CMS",
    "implies": [
        "Apache",
        "PHP"
    ],
    "matches": [
        {
            "search": "meta[generator]",
            "offset": 1,
            "regexp": "Danneo CMS ([\\d.]+)"
        },
        {
            "search": "headers[x-powered-by]",
            "offset": 1,
            "regexp": "CMS Danneo ([\\d.]+)"
        }
    ]
}