{
    "website": "http://gravityforms.com",
    "name": "Gravity Forms",
    "matches": [
        {
            "regexp": "<div class=(?:\"|')[^>]*gform_wrapper"
        },
        {
            "regexp": "<div class=(?:\"|')[^>]*gform_body"
        },
        {
            "regexp": "<ul [^>]*class=(?:\"|')[^>]*gform_fields"
        },
        {
            "regexp": "<link [^>]*href=(?:\"|')[^>]*wp-content/plugins/gravityforms/css/"
        }
    ]
}