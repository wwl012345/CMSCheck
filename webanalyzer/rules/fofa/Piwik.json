{
    "name": "Piwik",
    "author": "fofa",
    "version": "0.1.0",
    "matches": [
        {
            "search": "headers",
            "text": "PIWIK_SESSID "
        },
        {
            "search": "title",
            "regexp": "Piwik"
        },
        {
            "search": "body",
            "text": "index.php?module=Proxy"
        }
    ],
    "condition": "0 or (1 and 2)"
}