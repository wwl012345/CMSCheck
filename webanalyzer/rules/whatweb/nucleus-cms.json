        {
            "regexp": "(?-mix:<td>Admin-area <strong>path<\\/strong>:<\\/td>[\\s]+<td><input name=\"AdminPath\" size=\"60\" value=\"([^\"]+)\" \\/>)"
        },
        {
            "text": "<meta name=\"name\" content=\"My Nucleus CMS\" />"
        },
        {
            "regexp": "(?-mix:<small>Copyright \\| <a href=\"http:\\/\\/nucleuscms\\.org\">Nucleus CMS v([^\\s^>]+)<\\/a> \\|)",
            "offset": 1
        }
    ]
}
