            "offset": 1
        },
        {
            "certainty": 75,
            "regexp": "(?-mix:&copy; [\\d]{4} by <a href='http:\\/\\/www\\.g0lgs\\.co\\.uk\\/downloads\\.php'>G0LGS<\\/a><\\/td><\\/tr>)"
        }
    ]
}
