        {
            "text": "<a id=\"screencastLink\" href=\"http://www.hyperic.com/demo/screencasts.html\" target=\"_blank\" title=\"Screencasts\">"
        },
        {
            "search": "headers[location]",
            "regexp": "(?-mix:\\/app\\/login;jsessionid=)"
        }
    ]
}
