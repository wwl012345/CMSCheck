{
    "name": "HP-ProCurve-Switch",
    "author": "Brendan Coles <bcoles@gmail.com>, Andrew Horton",
    "version": "0.2",
    "description": "HP ProCurve Switch",
    "website": "http://www.hp.com/rnd/",
    "matches": [
        {
            "regexp": "(?-mix:^eHTTP)",
            "search": "headers[server]"
        },
        {
            "search": "headers[server]",
            "regexp": "(?-mix:^eHTTP v([\\d\\.]{1,3})$)",
            "offset": 1
        }
    ]
}
