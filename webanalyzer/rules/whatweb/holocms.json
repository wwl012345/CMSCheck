        {
            "text": "<div id=\"remember-me-notification\" class=\"bottom-bubble\" style=\"display:none;\">"
        },
        {
            "regexp": "(?-mix:^Powered by HoloCMS &copy[;]* 2008 Meth0d & Parts by Yifan, sisija)"
        }
    ]
}
