        {
            "offset": 2,
            "regexp": "(?-mix:<!-- (Lyris )?List[\\s]*Manager (WIN32|LINUX) version ([^\\/]+) (- [A-Z][a-z]{2} [\\d]{1,2} [\\d]{4})?( [\\d]{2}:[\\d]{2}:[\\d]{2})?\\/ lyrispg.pl version [^>]+ -->)"
