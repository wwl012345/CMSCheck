{
    "name": "Xerver",
    "author": "Brendan Coles <bcoles@gmail.com>, Andrew Horton",
    "version": "0.2",
    "description": "Xerver is an advanced free GNU GPL Web and FTP server.",
    "website": "http://www.javascript.nu/xerver/",
    "matches": [
        {
            "regexp": "(?-mix:^Xerver)",
            "search": "headers[server]"
        },
        {
            "search": "headers[server]",
            "regexp": "(?-mix:^Xerver\\/([\\d\\.]+)$)",
            "offset": 1
        }
    ]
}
