    "matches": [
        {
            "search": "headers[set-cookie]",
            "regexp": "(?-mix:markc)",
            "name": "markc cookie"
        },
        {
            "search": "headers[set-cookie]",
            "regexp": "(?-mix:^moonfruit)",
            "name": "moonfruit cookie"
        }
    ]
}
