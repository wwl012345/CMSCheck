{
    "name": "Oracle-HTTP-Server",
    "author": "Brendan Coles <bcoles@gmail.com>",
    "version": "0.1",
    "description": "Oracle HTTP Server",
    "website": "http://www.oracle.com/technetwork/middleware/ias/index-091236.html",
    "matches": [
        {
            "search": "headers[server]",
            "regexp": "(?-mix:Oracle[ -]HTTP[ -]Server)"
        },
        {
            "search": "headers[server]",
            "offset": 1,
            "regexp": "(?-mix:Oracle_Web_[Ll]istener(_NT_)?([\\d\\.]+\\/[^\\s]+))"
        },
        {
            "search": "headers[server]",
            "offset": 1,
            "regexp": "(?-mix:Oracle_Web_[Ll]istener(_NT_)?\\/([^\\s]+))"
        }
    ]
}
