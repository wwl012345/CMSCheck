{
    "name": "Monkey",
    "author": "Brendan Coles <bcoles@gmail.com>, Andrew Horton",
    "version": "0.2",
    "description": "Monkey is a very small, fast and scalable web server for Linux.",
    "website": "http://www.monkey-project.com/",
    "matches": [
        {
            "regexp": "(?-mix:^Monkey)",
            "search": "headers[server]"
        },
        {
            "search": "headers[server]",
            "regexp": "(?-mix:^Monkey\\/([\\d\\.]+))",
            "offset": 1
        }
    ]
}
