{
    "name": "ElasticSearch",
    "author": "Andrew Hortons",
    "version": "0.1",
    "description": "Elasticsearch is a distributed RESTful search engine built for the cloud. Built on top of Apache Lucene.",
    "website": "https://github.com/elastic/elasticsearch",
    "matches": [
        {
            "name": "Tagline",
            "url": "/",
            "status": 200,
            "text": "\"tagline\" : \"You Know, for Search\"\n}"
        },
        {
            "name": "JSON version.number",
            "url": "/",
            "status": 200,
            "regexp": "(?-mix:\"name\" : \"[^\"]+\",\\s+\"cluster_name\" : \"[^\"]+\",\\s+\"cluster_uuid\" : \"[^\"]+\",\\s+\"version\" : {\\s+\"number\" : \"([^\"]+)\")",
            "offset": 1
        }
    ]
}
