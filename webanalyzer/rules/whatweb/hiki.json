{
    "name": "Hiki",
    "author": "Brendan Coles <bcoles@gmail.com>",
    "version": "0.1",
    "description": "Hiki is a powerful and fast wiki clone written in Ruby.",
    "website": "http://hikiwiki.org/en/",
    "matches": [
        {
            "regexp": "(?-mix:<meta name=\"generator\" content=\"Hiki ([^\"^\\s]+)\">)",
            "offset": 1
        },
        {
            "text": "<html><head><title>Hiki Error</title></head><body>"
        },
        {
            "regexp": "(?-mix:<div class=\"footer\">Generated by <a href=\"http:\\/\\/hikiwiki.org\\/\">Hiki<\\/a> ([^\\s]+) \\([\\d]{4}-[\\d]{2}-[\\d]{2}\\))",
            "offset": 1
        }
    ]
}
