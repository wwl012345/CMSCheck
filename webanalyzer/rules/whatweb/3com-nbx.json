        {
            "regexp": "(?-mix:<span class=\"splashTitleNBX\">NBX<SUP><span class=\"splashTitleNBXReg\">&reg;<\\/span><\\/SUP><\\/span>[\\s]+<span class=\"splashTitlePlatform\">(&nbsp;)?([^<]+)<\\/span>)"
